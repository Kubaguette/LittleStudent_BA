{
  "expo": {
    "name": "LittleStudent",
    "slug": "little-student",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./components/pictures/littlestudent-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
