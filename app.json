      "adaptiveIcon": {
        "foregroundImage": "./components/pictures/littlestudent-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.kubaguette.LittleStudent"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
